{
  "name": "@bfemulator/app-shared",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "built/index.js",
  "types": "built/index.d.ts",
  "scripts": {
    "build": "npm run lint && npm run clean && npm run build:prod && tsc --emitDeclarationOnly --declaration",
    "build:prod": "babel ./src --out-dir built --extensions \".ts,.tsx\" --ignore \"**/*.spec.ts\"",
    "clean": "rimraf ./built",
    "prepare": "npm run clean",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@types/jest": "^22.2.3",
    "babel-jest": "23.6.0",
    "jest": "^23.0.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.10.0",
    "typescript": "3.1.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "@bfemulator/sdk-shared": "^1.0.0",
    "botframework-config": "4.0.0-preview1.3.4",
    "tslib": "^1.9.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "rootDir": "./src",
    "testMatch": [
      "**/?(*.)(spec|test).(ts)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
